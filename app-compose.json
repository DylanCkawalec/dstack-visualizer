{
  "manifest_version": 1,
  "name": "attestation-dashboard",
  "runner": "docker-compose",
  "allowed_envs": [
    "PHALA_API_KEY",
    "PHALA_ENDPOINT", 
    "PHALA_CLUSTER_ID",
    "PHALA_CONTRACT_ID",
    "APP_NAME",
    "DEVELOPER_NAME",
    "DEVELOPER_ROLE", 
    "ORGANIZATION",
    "NODE_ENV",
    "TEE_ENVIRONMENT",
    "ENABLE_MOCK_MODE",
    "REQUIRE_ATTESTATION",
    "ATTESTATION_SEED",
    "ATTESTATION_SALT",
    "PORT",
    "API_PORT", 
    "BUN_PORT"
  ],
  "public_tcbinfo": true,
  "public_sysinfo": true,
  "kms_enabled": true,
  "gateway_enabled": true
}
